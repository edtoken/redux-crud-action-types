{
  "name": "redux-crud-action-types",
  "version": "1.0.5",
  "description": "A simple lib help your define redux action type in easy way",
  "main": "./lib/redux-crud-action-types.js",
  "scripts": {
    "prepublish": "npm run build",
    "lint": "eslint --color src",
    "clean": "rm -rf lib",
    "lint:fix": "eslint --fix src --ext .js,.jsx src",
    "prebuild": "npm run clean && npm run lint && npm run test",
    "precommit": "npm run lint && npm run test",
    "build": "./node_modules/.bin/babel --presets es2015 -d lib src",
    "test": "mocha --compilers js:babel-register ./test"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/edtoken/redux-crud-action-types.git"
  },
  "keywords": [
    "redux",
    "react",
    "crud",
    "redux-crud",
    "redux-ajax",
    "ajax"
  ],
  "author": "Eduard Titov <editied@gmail.com>",
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.githubusercontent.com/edtoken/redux-crud-action-types/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/edtoken/redux-crud-action-types/issues"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^4.0.1",
    "should": "^13.1.1",
    "standart": "^6.1.0"
  }
}
